<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
           onKeyPressed="#handleKeyPress" prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quit" prefHeight="60.0" prefWidth="150.0" text="QUIT"
           StackPane.alignment="TOP_LEFT">
      <StackPane.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
      </StackPane.margin>
      <font>
         <Font size="28.0"/>
      </font>
   </Button>
</StackPane>
